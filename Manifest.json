{

  "description": "Simple Unitus Ticker - Based on Coin Market Cap",
  "manifest_version": 2,
  "name": "Unitus Price Ticker",
  "version": "1.0.2",
  "homepage_url": "https://github.com/mdn/webextensions-examples/tree/master/open-my-page-button",
  "icons": {
    "48": "icons/unitus-48.png"
  },
  "browser_style": false,
  "background": {
    "scripts": ["background.js"]
  },
  "browser_action": {
    "default_icon": "icons/unitus-32.png",
    "default_popup": "options.html"
  },
  "options_ui": {
    "page": "options.html"
  }
}